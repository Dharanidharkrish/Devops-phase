pipeline
{
    agent any
    
    tools{
    maven 'Maven'
    }
    
   stages{
    stage('Build')
        {
            steps{
              sh 'mvn clean install'
                 }
         }
     
     stage('SonarQube analysis') {
    withSonarQubeEnv('SonarQube') {
      sh 'mvn clean package sonar:sonar'
    }
   }
    
  }
}
