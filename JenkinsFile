pipeline
{
    agent any
    
    tools{
    maven 'Maven'
    }
    
   stages{
    stage('Build')
        {
            steps{
              sh 'mvn clean install'
                 }
         }
     
     stage('SonarQube analysis') {
       steps{
        withSonarQubeEnv('SonarQube') {
            sh 'mvn package sonar:sonar'
           }
       }
   }
   
    stage('Nexus')
    {
     steps{
   nexusArtifactUploader artifacts: [[artifactId: 'Square', classifier: '', file: 'pom.xml', type: 'jar']], credentialsId: 'e981b30a-0349-4076-9a25-bbd5607d10f7', groupId: 'Codekillers.artifact', nexusUrl: 'ec2-18-224-155-110.us-east-2.compute.amazonaws.com:8081/nexus', nexusVersion: 'nexus2', protocol: 'http', repository: 'devopstraining', version: '1.0'
        }
    }
   
    
  }
}
